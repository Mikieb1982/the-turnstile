{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",

    "baseUrl": "src",
    "paths": {},

    "strict": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": true,
    "noEmit": true,

    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["src"]
}
